app.controller('SeriesShowCtrl', ['$scope', '$http', '$location', '$routeParams', function($scope, $http, $location, $routeParams){

  var url = "<%= ENV['URL'] %>" || "http://localhost:3000";

  $http.get(url + '/series/' + $routeParams.id).success(function(response){
    $scope.serie = response;
  });
}]);