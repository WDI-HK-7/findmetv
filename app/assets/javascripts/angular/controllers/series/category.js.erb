app.controller('SeriesCategoryCtrl', ['$scope', '$http', '$location', '$routeParams', function($scope, $http, $location, $routeParams){

  $http.get("<%= ENV['URL'] %>" + 'findByCategory/' + $routeParams.category ).success(function(response){
    console.log(response);
    $scope.category = response;
  });
  
}]);