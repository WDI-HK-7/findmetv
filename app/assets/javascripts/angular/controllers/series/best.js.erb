app.controller('SeriesBestCtrl', ['$scope', '$http', '$location', function($scope, $http, $location){

  $http.get("<%= ENV['URL'] %>" + 'best').success(function(response){
    console.log(response);
    $scope.best = response;
  });

  $http.get("<%= ENV['URL'] %>" + 'worst').success(function(response){
    console.log(response);
    $scope.worst = response;
  });

}]);